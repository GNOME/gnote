#: ../src/sharp/addinstreemodel.cpp:138
msgid "Formatting"
msgstr "Formato"

#. TRANSLATORS: Addin category.
#: ../src/sharp/addinstreemodel.cpp:141
msgid "Desktop integration"
msgstr "Integración con el escritorio"

#. TRANSLATORS: Addin category.
