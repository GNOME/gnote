msgid "_File"
msgstr "_Arkiv"

#: ../src/actionmanager.cpp:178
msgid "_New"
msgstr "_Ny"

#: ../src/actionmanager.cpp:179 ../src/actionmanager.cpp:230
msgid "Create a new note"
msgstr "Skapa en ny anteckning"

#: ../src/actionmanager.cpp:184
msgid "_Open..."
msgstr "_Öppna..."

#: ../src/actionmanager.cpp:184
msgid "Open the selected note"
