msgctxt "shortcut window"
msgid "Note Window"
msgstr "Nova janela"

#: data/shortcuts-gnote.ui:155
msgctxt "shortcut window"
msgid "Open note actions menu"
msgstr "Abre o menu de ações da nota"

#: data/shortcuts-gnote.ui:184
msgctxt "shortcut window"
